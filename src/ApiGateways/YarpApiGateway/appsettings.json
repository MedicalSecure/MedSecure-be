{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "waste-route": {
        "ClusterId": "waste-cluster",
        "Match": {
          "Path": "/waste-service/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "diet-route": {
        "ClusterId": "diet-cluster",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/diet-service/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "bacpatient-route": {
        "ClusterId": "bacpatient-cluster",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/bacpatient-service/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    },
    "Clusters": {
      "waste-cluster": {
        "Destinations": {
          "wastedestination1": {
            "Address": "http://localhost:6002"
          }
        }
      },
      "diet-cluster": {
        "Destinations": {
          "dietdestination1": {
            "Address": "http://localhost:6003"
          }
        }
      },
      "bacpatient-cluster": {
        "Destinations": {
          "bacpatientdestination1": {
            "Address": "http://localhost:6005"
          }
        }
      }
    }
  }
}