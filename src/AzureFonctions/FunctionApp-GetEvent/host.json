{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxConcurrentRequests": 100,
      "maxOutstandingRequests": 200,
      "dynamicThrottlesEnabled": false
    }
  },
  "http": {
    "cors": {
      "origins": [ "*" ],
      "headers": [ "*" ],
      "methods": [ "GET", "POST" ]
    }
  }
}
